    echo "wunderground station id       $C_WS_WUNDERGROUND_ID
wunderground station password $C_WS_WUNDERGROUND_PASSWORD"
}

parseWunderground() {
    readString
    C_WS_WUNDERGROUND_ID=$VALUE_STRING
    readString
    C_WS_WUNDERGROUND_PASSWORD=$VALUE_STRING

    printWunderground
}

